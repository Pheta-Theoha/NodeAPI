      nav(class="nav is-active" id="nav")
        a(href="/tourismMan/tServices" class="nav-link" class="path-nav") Services
        a(href="/tourismMan/tAbout" class="nav-link" class="path-nav") About
        a(href="/tourismMan/tContact" class="nav-link" class="path-nav") Contact
        a(href="/admin/adminHome" class="nav-link" id="admin" class="material-icons") home
    script(src="../public/javascripts/main.js")
    block content

